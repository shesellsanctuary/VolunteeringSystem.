@model IEnumerable<Administrator>
@{
    ViewData["Title"] = "Administradores " + ViewBag.Status;
    Layout = "_LayoutAdmin";
}

<h2>
	Administradores
	<a href="@Url.Action("Add", "Admin")" class="btn btn-success pull-right">Criar Novo</a>
</h2>

<div class="panel panel-default">
    <table class="table">
        <tr>
            <th>Nome</th>
            <th>CPF</th>
            <th>Contato</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.name</td>
                <td>@item.CPF</td>
                <td>@item.credentials.email</td>
            </tr>
        }
    </table>
</div>